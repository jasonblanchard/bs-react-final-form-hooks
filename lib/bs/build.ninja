bs_package_flags = -bs-package-name react-hooks-template -bs-package-map ReactHooksTemplate
src_root_dir = /Users/jblanchard/projects/bs-react-final-form-hooks
bsc = /Users/jblanchard/projects/bs-react-final-form-hooks/node_modules/bs-platform/lib/bsc.exe
bsdep = /Users/jblanchard/projects/bs-react-final-form-hooks/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -bs-suffix -nostdlib -I '/Users/jblanchard/projects/bs-react-final-form-hooks/node_modules/bs-platform/lib/ocaml' -color always
ppx_flags = 
bs_package_includes = -I /Users/jblanchard/projects/bs-react-final-form-hooks/node_modules/reason-react/lib/ocaml
bs_package_dev_includes = 
namespace = -ns ReactHooksTemplate
bsb_dir_group = 0
refmt = /Users/jblanchard/projects/bs-react-final-form-hooks/node_modules/bs-platform/lib/refmt.exe
reason_react_jsx = -bs-jsx 3
refmt_flags = --print binary
bsc_lib_includes = -I . -I examples -I src
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/ReactFinalFormHooks.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/ReactFinalFormHooks.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build src/ReactFinalFormHooks.mlast.d : build_deps src/ReactFinalFormHooks.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} ${gentypeconfig} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build src/ReactFinalFormHooks-ReactHooksTemplate.cmj |  $src_root_dir/lib/js/src/ReactFinalFormHooks.bs.js src/ReactFinalFormHooks-ReactHooksTemplate.cmi : build_cmj_cmi src/ReactFinalFormHooks.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
